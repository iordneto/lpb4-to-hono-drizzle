{
  "name": "loopback-prisma-to-hono-drizzle",
  "version": "0.0.1",
  "description": "loopback prisma to hono drizzle",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "test:watch": "yarn run test:dev",
    "test:tap": "yarn run test --reporter=tap",
    "test:auth": "yarn run test --grep \"AuthController\"",
    "test:tasks": "yarn run test --grep \"TaskController\"",
    "test:unit": "lb-mocha --allow-console-logs \"dist/__tests__/unit\"",
    "test:integration": "lb-mocha --allow-console-logs \"dist/__tests__/acceptance\"",
    "test:coverage": "c8 --reporter=text --reporter=html yarn run test:integration",
    "test:coverage:report": "c8 report --reporter=html --reporter=text",
    "test:coverage:check": "c8 check-coverage --lines 80 --functions 80 --branches 80 --statements 80",
    "docker:build": "docker build -t loopback-prisma-to-hono-drizzle .",
    "docker:run": "docker run -p 3000:3000 -d loopback-prisma-to-hono-drizzle",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build",
    "db:start": "docker-compose up -d postgres",
    "db:stop": "docker-compose down",
    "db:logs": "docker-compose logs -f postgres",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres",
    "adminer:start": "docker-compose up -d adminer",
    "dev": "yarn db:start && yarn start",
    "dev:full": "docker-compose up -d && yarn start"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist/__tests__/**/*",
      "dist/migrate.js",
      "dist/index.js"
    ],
    "all": true,
    "check-coverage": false,
    "lines": 80,
    "functions": 80,
    "branches": 80,
    "statements": 80
  },
  "author": "iordneto <iordneto95@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^12.0.3",
    "@loopback/authentication-jwt": "^0.16.3",
    "@loopback/boot": "^8.0.2",
    "@loopback/core": "^7.0.1",
    "@loopback/repository": "^8.0.1",
    "@loopback/rest": "^15.0.2",
    "@loopback/rest-explorer": "^8.0.2",
    "@loopback/security": "^0.12.2",
    "@loopback/service-proxy": "^8.0.1",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.0",
    "jsonwebtoken": "^9.0.2",
    "loopback-connector-postgresql": "^7.2.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@loopback/build": "^12.0.1",
    "@loopback/eslint-config": "^16.0.0",
    "@loopback/testlab": "^8.0.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^16.18.126",
    "c8": "^10.1.3",
    "eslint": "^8.57.1",
    "nyc": "^17.1.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
